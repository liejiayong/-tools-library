.center-header{
    position: relative;
    padding: 20rpx 40rpx 30rpx;
    align-items: center;
}
.account-avatar{
    width: 124rpx;
    height: 124rpx;
    background-color: #f5f5f5;
    border-radius: 50%;
}
.account-profile{
    position: relative;
    margin-top: 40rpx;
}
.account-name{
    word-break: break-all;
    word-wrap: break-word;
    font-size: 40rpx;
    line-height: initial;
    vertical-align: middle;
}
.account-medal{
    margin-left: 20rpx;
    width: 76rpx;
    height: 36rpx;
    /* background-color: #f5f5f5; */
    vertical-align: middle;
}
.jy-desc{
    margin-top: 25rpx;
    align-items: center;
}
.jy-focus{
    position: absolute;
    right: 40rpx;
    bottom: 75rpx;
}
.desc-item{
    padding: 0 20rpx;
    border-right: 1rpx solid #E6E6E6;
    font-size: 24rpx;
    color: #999;
}
.desc-item:first-of-type{
    padding-left: 0;
}
.desc-item:last-of-type{
    border-right: none;
    padding-right: 0;
}
.desc-count{
    font-size: 36rpx;
}
.jy-post{
    padding: 0 40rpx;
    border-top: 10rpx solid #f1f2f3;
}
.post-count{
    height: 88rpx;
    line-height: 88rpx;
    font-size: 24rpx;
    color: #333;
}
