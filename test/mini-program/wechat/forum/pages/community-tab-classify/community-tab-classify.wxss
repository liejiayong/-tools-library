.jy-community-tab-classify {
    padding: 27rpx 0;
    align-items: center;
    border-bottom: 1rpx solid #F2F2F2;
    cursor: pointer;
}
.tab-avatar {
  flex: 0 0 80rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
}
.tab-txt {
  flex: 1;
  text-indent: 15rpx;
  max-width: 550rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 24rpx;
  font-weight: bold;
  color: #1a1a1a;
}
.btn-community-post{
    position: fixed;
    bottom: 180rpx;
    right: 40rpx;
    width: 90rpx;
    height: 90rpx;
    z-index: 100;
  }