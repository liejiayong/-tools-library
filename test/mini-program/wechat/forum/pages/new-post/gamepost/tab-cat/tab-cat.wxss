.tabcatindex{
  position: relative;
  display: flex;
  padding: 22rpx 0;
  flex-wrap: wrap;
  width: 100%;
  height: 210rpx;
  overflow: hidden;
}
.jy-flex-item-25 {
  -webkit-box-flex: 0 0 25%;
  -webkit-flex: 0 0 25%;
  -moz-box-flex: 0;
  -ms-flex: 0 0 25%;
  flex: 0 0 25%;
  width: 25%;
  height: inherit;
  text-align: center;
  font-size:0;
  box-sizing: border-box;
  cursor: pointer;
}
.avatar{
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.text{
  margin-top: 24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 24rpx;
  font-weight: bold;
  color: #1A1A1A;
}